{
  "name": "react-router-breadcrumbs-hoc",
  "version": "4.0.0",
  "description": "small, flexible, higher order component for rendering breadcrumbs with react-router 4.x",
  "repository": "icd2k3/react-router-breadcrumbs-hoc",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "umd": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "test-build": "sh ./scripts/test-build.sh",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint ./src/**"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn build && yarn lint && yarn test",
      "pre-push": "yarn build && yarn types && yarn lint && yarn test-build"
    }
  },
  "author": "Justin Schrader (me@justin.beer)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8",
    "react-router": ">=4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "add": "^2.0.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-router": "^5.2.0",
    "rollup": "^2.1.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.8.3",
    "yarn": "^1.22.4"
  },
  "resolutions": {
    "@types/react": "^16.9.23"
  },
  "keywords": [
    "react",
    "router",
    "breadcrumbs",
    "react-router",
    "react-router 4",
    "react-router 5",
    "react-router 6"
  ]
}

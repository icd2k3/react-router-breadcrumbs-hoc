{
  "name": "react-router-breadcrumbs-hoc",
  "version": "3.2.5",
  "description": "small, flexible, higher order component for rendering breadcrumbs with react-router 4.x",
  "repository": "icd2k3/react-router-breadcrumbs-hoc",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "umd": "dist/umd/index.js",
  "types": "types/react-router-breadcrumbs-hoc/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "test-build": "sh ./scripts/test-build.sh",
    "types": "yarn type-src && yarn type-descriptions",
    "type-src": "tsc -p tsconfig.json",
    "type-descriptions": "tsc -p types/react-router-breadcrumbs-hoc",
    "travis": "sh ./scripts/travis.sh",
    "lint": "eslint ./src/**"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn build && yarn lint && yarn test",
      "pre-push": "yarn build && yarn types && yarn lint && yarn test-build"
    }
  },
  "author": "Justin Schrader (me@justin.beer)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16",
    "react-router": ">=4",
    "react-router-dom": ">=4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/react": "^16.9.19",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "react",
    "router",
    "breadcrumbs",
    "react-router",
    "react-router 4"
  ]
}

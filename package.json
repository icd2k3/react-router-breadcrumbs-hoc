{
  "name": "react-router-breadcrumbs-hoc",
  "version": "3.2.0",
  "description": "small, flexible, higher order component for rendering breadcrumbs with react-router 4.x",
  "repository": "icd2k3/react-router-breadcrumbs-hoc",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "umd": "dist/umd/index.js",
  "types": "types/react-router-breadcrumbs-hoc/index.d.ts",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use yarn to install dependencies, not npm')\"",
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "test-build": "sh ./scripts/test-build.sh",
    "types": "tsc -p types/react-router-breadcrumbs-hoc",
    "travis": "sh ./scripts/travis.sh",
    "lint": "eslint ./src/**"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn types && yarn lint && yarn test-build"
    }
  },
  "author": "Justin Schrader (me@justin.beer)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16",
    "react-router": ">=4",
    "react-router-dom": ">=4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/react": "^16.8.19",
    "@types/react-router-dom": "^4.3.3",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "js-yaml": "^3.13.1",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.5.1"
  },
  "keywords": [
    "react",
    "router",
    "breadcrumbs",
    "react-router",
    "react-router 4"
  ]
}
